.grid-folder-container
  box-sizing border-box
  padding 5px 15px
  width 200px
  height 50px
  display flex
  cursor pointer
  justify-content space-between
  align-items center
  border 1px solid black
  &:hover
    background #D9D9D9
  .type-icon
    margin-right 10px
  .name
    margin 0 15px 0 0
    font-size 18px
    max-width 130px
    overflow hidden
    text-overflow ellipsis
    white-space nowrap
